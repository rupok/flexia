{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "title": "Ultra Marine",
  "settings": {
    "color": {
      "palette": [
      
        {
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#000000",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#acacd0",
					"name": "Contrast /Two",
					"slug": "contrast-2"
				},
				{
					"color": "#F1F3F5",
					"name": "Contrast /three",
					"slug": "contrast-3"
				},
				{
					"color": "#e7e5e5",
					"name": "Contrast / Four",
					"slug": "contrast-4"
				},
        
				{
					"color": "#ceddf7",
					"name": "Contrast / Four",
					"slug": "contrast-5"
				},

        {
          "name": "Primary",
          "color": "#286DF6",
          "slug": "primary"
        },
        {
          "name": "Secondary",
          "color": "#043699",
          "slug": "secondary"
        },
        {
          "name": "Background",
          "color": "#F8FAFC",
          "slug": "background"
        },
				{
					"color": "#F9556D",
					"name": "Primary /Two",
					"slug": "primary-2"
				},
				{
					"name": "Transparent",
					"color": "transparent",
					"slug": "transparent"
				  }
       
      ],
      "gradients": [
				{
					"slug": "primary",
					"gradient": "linear-gradient(286.83deg, #0ABEEB -1.55%, #0F26F7 100%)",
					"name": "Primary"
				}
      ],
    
        
      "duotone": [
				{
					"colors": [
						"#286DF6",
						"#286DF6"
					],
					"name": "Dark Grayscale",
					"slug": "dark-gray"
				},
				
				{
					"colors": [
						"#000097",
						"#148488"
					],
					"name": "Dark Green",
					"slug": "dark-green"
				},
				
				{
					"colors": [
						"#8c00b7",
						"#f11bad"
					],
					"name": "Dark Pink",
					"slug": "dark-pink"
				},
				
				{
					"colors": [
						"#000097",
						"#1616b1"
					],
					"name": "Royal Blue",
					"slug": "royal-blue"
				},
				
				{
					"colors": [
						"#1900d8",
						"#f40909"
					],
					"name": "Dark Red",
					"slug": "dark-red"
				},
				
				{
					"colors": [
						"#000000",
						"#00a5ff"
					],
					"name": "Dark Blue",
					"slug": "dark-blue"
				}

			]
    }
  },
  "styles": {

    "color": {
      "background": "var(--wp--preset--color--base)",
      "text": "var(--wp--preset--color--contrast)"
    },

    "elements": {

      "button": {
        ":active": {
          "color": {
            "background": "var(--wp--preset--color--primary)",
            "text": "var(--wp--preset--color--base)"
          }
        },
        ":focus": {
          "color": {
            "background": "var(--wp--preset--color--contrast)",
            "text": "var(--wp--preset--color--base)"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary-2)",
            "text": "var(--wp--preset--color--white)"
          }
        },
        ":visited": {
          "color": {
            "text": "var(--wp--preset--color--black)"
          }
        },
        "color": {
          "background": "var(--wp--preset--color--primary)",
          "text": "var(--wp--preset--color--white)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontSize": "16px",
          "fontStyle": "normal",
          "fontWeight": "600",
          "textTransform": "capitalize"
        }
      },
      "heading": {
        "color": {
          "text": "var(--wp--preset--color--black)"
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--sora)",
          "fontStyle": "normal",
          "fontWeight": "600",
          "letterSpacing": "-.5px",
          "lineHeight": "1.2",
          "textTransform": "normal"
        }
      },
      "link": {
        ":active": {
          "color": {
            "text": "var(--wp--preset--color--secondary)"
          }
         
        },
        ":focus": {
          "typography": {
            "textDecoration": "underline dashed"
          }
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary)"
          }
        },
        "color": {
          "text": "var(--wp--preset--color--contrast)"
        }
      }
    }
  }

  
}