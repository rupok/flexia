					"name": "Dark Grayscale",
					"slug": "dark-gray"
				},
				{
					"colors": [
						"#000000",
						"#00A5FF"
					],
					"name": "Midnight Blue",
					"slug": "midnight-blue"
				}
			],
			"gradients": [
				{
					"slug": "primary",
					"gradient": "background: linear-gradient(270deg, #FFB400 0.04%, #FF3F3F 99.95%), #D9D9D9",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"gradient": "linear-gradient(270deg, #d9e0e2 -5%, #c4bfd9 100%)",
					"name": "Secondary"
				},
				{
					"slug": "vibrant",
					"gradient": " linear-gradient(270deg, #F1F6FF 8.98%, #FF3F3F 99.96%), #D9D9D9 ",
					"name": "Vibrant"
				},
				{
					"slug": "primary-trans",
					"gradient": "linear-gradient(0deg, #001847 0%, rgba(0, 87, 255, 0.00) 102.42%)",
					"name": "Primary Transparent"
				},
				{
					"slug": "primary-grad",
					"gradient": " conic-gradient(from -29deg at 8.97% 20.54%, #FFABAB 0deg, #FEECEC 177.64721989631653deg)",
					"name": "Primary Gradient"
				},
				{
					"slug": "secondary-grad",
					"gradient": "conic-gradient(from -29deg at 8.97% 20.54%, #FFEFC5 0deg, #FBFAF0 177.64721989631653deg)",
					"name": "Secondary Gradient"
					
				}
			],
			"palette": [
				{
					"color": "#FF3F3F",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#F7FCFF",
					"name": "Primary Trans",
					"slug": "primary-trans-color"
				},

				{
					"color": "#FFF3F3",
					"name": "Secondary",
					"slug": "secondary"
				},

				{
					"color": "#EEF9FE",
					"name": "Secondary BG",
					"slug": "secondary-bg"
				},

				{
					"color": "#fff",
					"name": "Text White",
					"slug": "body-white"
				},
				{
					"color": "#fff",
					"name": "White BG",
					"slug": "white-bg"
				},
				{
					"color": "#FFFCF3",
					"name": "Background Alt",
					"slug": "background-alt"
				},
				{
					"color": "#FFB400",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#031E53",
					"name": "Button Background",
					"slug": "button-background"
				},
				{
					"color": "#FFFFFF",
					"name": "Button-text",
					"slug": "button-text"
				},
				{
					"color": "#FFFFFF",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#515151",
					"name": "Text",
					"slug": "body"
				},
				{
					"color": "#FF3F3F",
					"name": "Link",
					"slug": "link-text"
				},
				{
					"color": "#202124",
					"name": "Heading",
					"slug": "heading-text"
				},
				{
					"color": "#FFFFFF",
					"name": "Light Background",
					"slug": "light-background"
				},
				{
					"color": "#C6D0E0",
					"name": "Text Alt",
					"slug": "body-alt"
				},
				{
					"color": "#001132",
					"name": "Dark Background",
					"slug": "dark-background"
				},
				{
					"color": "#F0F5FB",
					"name": "Border Color",
					"slug": "border-color"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"color": {
					"text": "var(--wp--preset--color--heading-text)"
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)!important"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--heading-text)"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/navigation-submenu": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"core/site-logo": {
				"filter": {
					"duotone": "var(--wp--preset--duotone--midnight-blue)"
				}
			},
			"core/query-pagination": {
				"elements": {
					"link": {
						":hover": {
							"border": {
								"color": "var(--wp--preset--color--contrast)"
							},
							"color": {
								"background": "var(--wp--preset--color--tertiary)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"border": {
								"color": "var(--wp--preset--color--body)",
								"width": "0 0 2px 0"
							}
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "#DFEDF0"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"border": {
							"color": "transparent"
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						":hover": {
							"border": {
								"color": "var(--wp--preset--color--primary)"
							},
							"color": {
								"background": "transparent"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"border": {
								"color": "var(--wp--preset--color--primary)"
							},
							"color": {
								"background": "transparent"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"elements": {
			"button": {
				"border": {
					"radius": "8px"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--button-text)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--button-background)",
					"text": "var(--wp--preset--color--button-text)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--heading-text)"
				}
			},
			"link": {
				":active": {
					"color": {
						"text": "var(--wp--preset--color--secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--link-text)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--body)"
		},
		"spacing": {
			"padding": {
				"top": "0",
				"right": "0",
				"bottom": "0",
				"left": "0"
			}
		}
	}
}