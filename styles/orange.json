{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Orange",
	"settings": {
		"color": {
			"duotone": [
				{
					"colors": [
						"#FF3F3F",
						"#FF3F3F"
					],
					"name": "Primary",
					"slug": "primary"
				},
				{
					"colors": [
						"#F9FEFF",
