				}
			],
			"gradients": [
				{
					"slug": "primary",
					"gradient": "linear-gradient(270deg, #0057FF -5%, #4F2CDD 100%)",
					"name": "Primary"
					
				},
				{
					"slug": "secondary",
					"gradient": "linear-gradient(270deg, #d9e0e2 -5%, #c4bfd9 100%)",
					"name": "Secondary"
					
				},
				{
					"slug": "vibrant",
					"gradient": "linear-gradient(270deg, #DEFFD2 0%, #41E5BE 100%)",
					"name": "Vibrant"
					
				},
				{
					"slug": "primary-trans",
					"gradient": "linear-gradient(0deg, #001847 0%, rgba(0, 87, 255, 0.00) 102.42%)",
					"name": "Primary Transparent"
				}

			],
			"palette": [
				{
					"color": "#0057FF",
					"name": "Primary",
					"slug": "primary"
				},
				{
